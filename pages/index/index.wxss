swiper{
  width: 100%;
  height: 340rpx;
}
swiper image{
  display: block;
  width: 100%;
  height: 100%;
}

/* items-list */
.items-list{
  
}
.pro-body{
  overflow: hidden;
}
.pro-item{
  position: relative;
  overflow: hidden;
  padding: 30rpx;
  border-bottom: 1px solid rgb(224, 216, 216);
}
.pro-title{
  margin:30rpx 0;
  color: #2e5e86;
  font-size: 34rpx;
  line-height: 1;
}
.pro-desc{
  overflow: hidden;
  font-size: 28rpx;
  color: #9a9a0a;
  line-height: 1;
}
.pro-footer{
  color:grey;
  bottom: 10rpx;
  right:10rpx;
  position:absolute;
  font-size:20rpx;
}